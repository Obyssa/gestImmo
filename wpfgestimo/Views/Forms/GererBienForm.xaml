        <Frame x:Name="frmAjout" Content="" HorizontalAlignment="Left" Height="314" VerticalAlignment="Top" Width="364" Margin="10,106,0,0"/>
        <Label x:Name="lblBiens" Content="Ajouter Un Bien" HorizontalAlignment="Left" Height="35" Margin="136,26,0,0" VerticalAlignment="Top" Width="112" FontWeight="Bold"/>
    </Grid>
</Page>
