      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:wpfgestimo.Views.Forms"
      mc:Ignorable="d" 
      d:DesignHeight="430" d:DesignWidth="390"
      Title="GererBienForm">

    <Grid>

        <Frame Content="" HorizontalAlignment="Left" Height="82" VerticalAlignment="Top" Width="364"/>

        <ComboBox x:Name="cbBoxBien" HorizontalAlignment="Center" Height="28" Margin="0,66,0,0" VerticalAlignment="Top" Width="194" SelectionChanged="cbBoxBien_SelectionChanged">
