<Page x:Class="GestImmo.Views.SubViews.ModifierBien"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GestImmo.Views.SubViews"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ModifierBien">

    <Grid>
        <TextBox x:Name="txtBoxNom" HorizontalAlignment="Left" Height="32" Margin="59,30,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="129" TextChanged="txtBoxNom_TextChanged" />
        <TextBox x:Name="txtBoxValeur" HorizontalAlignment="Left" Height="35" Margin="59,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="129"/>
        <TextBox x:Name="txtBoxAdresse" HorizontalAlignment="Left" Height="34" Margin="59,107,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="129"/>
        <TextBox x:Name="txtBoxSurface" HorizontalAlignment="Left" Height="35" Margin="59,146,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="129"/>
        <Button x:Name="btnModife" Content="Modifier" HorizontalAlignment="Left" Margin="99,205,0,0" VerticalAlignment="Top" Click="btnModife_Click"/>

    </Grid>
</Page>
